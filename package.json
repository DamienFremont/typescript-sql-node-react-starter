{
  "name": "my-app-client",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-intl": "^2.4.0",
    "react-intl-universal": "^1.13.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "reactstrap": "^6.4.0"
  },
  "scripts": {
    "start": "node dist/server/index.js",
    "build": "concurrently --kill-others-on-fail \"react-scripts-ts build\" \"tsc -p src/server\"",
    "dev": "concurrently --kill-others-on-fail \"react-scripts-ts start\" \"tsc -w -p src/server\"  \"nodemon dist/server/index.js\"",
    "dev:server": "concurrently --kill-others-on-fail \"tsc -w -p src/server\" \"nodemon dist/server/index.js\"",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "heroku-postbuild": "npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.0",
    "@types/jest": "^23.3.2",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.9.4",
    "@types/passport": "^0.4.6",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-intl": "^2.3.10",
    "@types/react-router-dom": "^4.3.1",
    "@types/reactstrap": "^6.0.2",
    "concurrently": "^3.5.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "nodemon": "^1.18.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "typescript": "^3.0.3",
    "winston": "^3.1.0"
  }
}
