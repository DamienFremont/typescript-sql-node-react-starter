{
  "name": "my-app-client",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@types/react-jsonschema-form": "^1.0.10",
    "ajv": "^6.5.4",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.2",
    "lodash": "^4.17.11",
    "react": "^16.3.2",
    "react-bootstrap-table": "^4.3.1",
    "react-dom": "^16.3.2",
    "react-intl": "^2.4.0",
    "react-intl-universal": "^1.13.1",
    "react-jsonschema-form": "^1.0.5",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "reactstrap": "^6.4.0"
  },
  "scripts": {
    "start": "node dist/server/index.js",
    "build": "concurrently --kill-others-on-fail \"react-scripts-ts build\" \"tsc -p src/server\"",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node,React\" -c \"yellow.bold,cyan.bold,green.bold,blue.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\" \"react-scripts-ts start\"",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "watch-ts": "tsc -w -p src/server",
    "watch-node": "nodemon dist/server/index.js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "heroku-postbuild": "npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.117",
    "@types/node": "^10.11.4",
    "@types/passport": "^0.4.6",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.4.13",
    "@types/react-bootstrap-table": "^4.3.5",
    "@types/react-dom": "^16.0.7",
    "@types/react-intl": "^2.3.10",
    "@types/react-router-dom": "^4.3.1",
    "@types/reactstrap": "^6.0.2",
    "@types/sequelize": "^4.27.27",
    "@types/swagger-ui-express": "^3.0.0",
    "concurrently": "^3.5.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sequelize": "^4.39.0",
    "sqlite3": "^4.0.2",
    "swagger-ui-express": "^4.0.1",
    "typescript": "^3.0.3",
    "winston": "^3.1.0"
  }
}
